# Rename this file to .env for local development. Values here will be loaded
# automatically if no other .env* file is present.
#
# Firebase client configuration
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_APP_ID=your-firebase-app-id
FIREBASE_MEASUREMENT_ID=
FIREBASE_MESSAGING_SENDER_ID=

# Firebase Admin / token verification
# Paste the JSON key set retrieved from https://www.googleapis.com/robot/v1/metadata/x509/securetoken@system.gserviceaccount.com
FIREBASE_AUTH_PUBLIC_KEYS={}

# Database configuration
DB_SYNC_FORCE=false
DB_SYNC_ALTER=true
